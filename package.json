{
  "name": "monarch",
  "version": "0.0.1",
  "scripts": {
    "install-eth-lightwallet": "gulp install-eth-lightwallet",
    "truffle-migrate": "cd truffle && truffle migrate",
    "truffle-compile": "cd truffle && truffle compile",
    "truffle-build": "cd truffle && truffle build",
    "truffle-all": "npm run truffle-compile && npm run truffle-build && npm run truffle-migrate && gulp truffle-sync",
    "truffle-serve": "npm run truffle-all && gulp install-eth-lightwallet && npm run clean-serve",
    "clean-serve": "clear && gulp clean && gulp serve",
    "test": "gulp test",
    "ipfs-daemon": "ipfs daemon",
    "dev": "concurrently --kill-others 'npm run ipfs-daemon' 'npm run clean-serve'"
  },
  "devDependencies": {
    "awesome-typescript-loader": "~0.14.0",
    "browser-sync": "~2.9.11",
