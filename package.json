    "node": ">=0.10.0"
  },
  "dependencies": {
    "asynch": "^0.5.2",
    "eth-lightwallet": "^2.5.3",
    "hooked-web3-provider": "^1.0.0",
    "ipfs-api": "^8.0.3",
    "uport-lib": "^1.3.0"
